	<xsd:simpleType name="_CSSGridLineType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="auto|inherit|((span)?(\s)?-?[0-9]+(\s/\s(span)?(\s)?-?[0-9]*)?)?"/>
